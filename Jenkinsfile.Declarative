pipeline {

def LABEL_ID = "web-app-${UUID.randomUUID().toString()}"   

  agent {
    kubernetes {
      cloud: 'k8s'
      label 'LABEL_ID'
      idleMinutes 5
      defaultContainer 'jnlp'
    }
  }
  stages {
    stage('Build') {
      steps {  // no container directive is needed as the maven container is the default
        sh "ola a todos"   
      }
    }
  }
}
